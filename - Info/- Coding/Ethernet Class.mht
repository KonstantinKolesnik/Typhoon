<DIV class=3Dcode><PRE xml:space=3D"preserve"><SPAN =
class=3Dhighlight-comment>/// Add these libraries to your project's =
References</SPAN>
<SPAN class=3Dhighlight-comment>/// System</SPAN>
<SPAN class=3Dhighlight-comment>/// Microsoft.SPOT.Net</SPAN>
<SPAN class=3Dhighlight-comment>/// GHIElectronics.NETMF.System</SPAN>
<SPAN class=3Dhighlight-comment>/// GHIElectronics.NETMF.Net</SPAN>
<SPAN class=3Dhighlight-keyword>using</SPAN> System;
<SPAN class=3Dhighlight-keyword>using</SPAN> System.Threading;
<SPAN class=3Dhighlight-keyword>using</SPAN> Microsoft.SPOT;
<SPAN class=3Dhighlight-keyword>using</SPAN> =
Microsoft.SPOT.Net.NetworkInformation;
<SPAN class=3Dhighlight-keyword>using</SPAN> GHIElectronics.NETMF.Net;
<SPAN class=3Dhighlight-keyword>using</SPAN> System.Net;

<SPAN class=3Dhighlight-keyword>namespace</SPAN> Ethernet_Example_4._0
{
    <SPAN class=3Dhighlight-keyword>public</SPAN> <SPAN =
class=3Dhighlight-keyword>class</SPAN> Program
    {
        <SPAN class=3Dhighlight-keyword>static</SPAN> <SPAN =
class=3Dhighlight-keyword>public</SPAN> <SPAN =
class=3Dhighlight-keyword>bool</SPAN> ethernet_event =3D <SPAN =
class=3Dhighlight-keyword>false</SPAN>;
        <SPAN class=3Dhighlight-keyword>static</SPAN> <SPAN =
class=3Dhighlight-keyword>public</SPAN> <SPAN =
class=3Dhighlight-keyword>bool</SPAN> ethernet_last_status =3D <SPAN =
class=3Dhighlight-keyword>false</SPAN>;
        <SPAN class=3Dhighlight-keyword>static</SPAN> <SPAN =
class=3Dhighlight-keyword>public</SPAN> <SPAN =
class=3Dhighlight-keyword>bool</SPAN> network_is_read =3D <SPAN =
class=3Dhighlight-keyword>false</SPAN>;
        <SPAN class=3Dhighlight-keyword>static</SPAN> <SPAN =
class=3Dhighlight-keyword>public</SPAN> ManualResetEvent =
NetworkAvailablityBlocking =3D <SPAN =
class=3Dhighlight-keyword>null</SPAN>;
        <SPAN class=3Dhighlight-keyword>public</SPAN> <SPAN =
class=3Dhighlight-keyword>static</SPAN> <SPAN =
class=3Dhighlight-keyword>void</SPAN> Main()
        {

            <SPAN class=3Dhighlight-keyword>if</SPAN> =
(!Ethernet.IsEnabled)
            {
                Ethernet.Enable();
            }
            NetworkChange.NetworkAvailabilityChanged +=3D <SPAN =
class=3Dhighlight-keyword>new</SPAN> =
NetworkAvailabilityChangedEventHandler(NetworkChange_NetworkAvailabilityC=
hanged);
            NetworkInterface[] netif =3D =
NetworkInterface.GetAllNetworkInterfaces();
            <SPAN class=3Dhighlight-comment>// Ethernet is always the =
first interface netif[0]</SPAN>
            NetworkAvailablityBlocking =3D <SPAN =
class=3Dhighlight-keyword>new</SPAN> ManualResetEvent(<SPAN =
class=3Dhighlight-keyword>false</SPAN>);
            <SPAN class=3Dhighlight-keyword>if</SPAN> =
(!Ethernet.IsCableConnected)
            {
                Debug.Print(<SPAN class=3Dhighlight-literal>"Cable is =
not connected!"</SPAN>);
                NetworkAvailablityBlocking.Reset();
                <SPAN class=3Dhighlight-keyword>while</SPAN> =
(!NetworkAvailablityBlocking.WaitOne(<SPAN =
class=3Dhighlight-number>5000</SPAN>, <SPAN =
class=3Dhighlight-keyword>false</SPAN>))
                {
                    <SPAN class=3Dhighlight-keyword>if</SPAN> =
(!Ethernet.IsCableConnected)
                    {
                        Debug.Print(<SPAN =
class=3Dhighlight-literal>"Cable is not connected!"</SPAN>);
                        Debug.Print(<SPAN =
class=3Dhighlight-literal>"Still waiting."</SPAN>);
                    }
                    <SPAN class=3Dhighlight-keyword>else</SPAN>
                        <SPAN class=3Dhighlight-keyword>break</SPAN>;
                }
            }

            Debug.Print(<SPAN class=3Dhighlight-literal>"Ethernet cable =
is connected!"</SPAN>);
            Debug.Print(<SPAN class=3Dhighlight-literal>"Enable =
DHCP"</SPAN>);
            <SPAN class=3Dhighlight-keyword>try</SPAN>
            {
                <SPAN class=3Dhighlight-keyword>if</SPAN> (!netif[<SPAN =
class=3Dhighlight-number>0</SPAN>].IsDhcpEnabled)
                    netif[<SPAN =
class=3Dhighlight-number>0</SPAN>].EnableDhcp();<SPAN =
class=3Dhighlight-comment>// This function is blocking</SPAN>
                <SPAN class=3Dhighlight-keyword>else</SPAN>
                {
                    netif[<SPAN =
class=3Dhighlight-number>0</SPAN>].RenewDhcpLease();<SPAN =
class=3Dhighlight-comment>// This function is blocking</SPAN>
                }
                network_is_read =3D <SPAN =
class=3Dhighlight-keyword>true</SPAN>;
                Debug.Print(<SPAN class=3Dhighlight-literal>"Network =
settings:"</SPAN>);
                Debug.Print(<SPAN class=3Dhighlight-literal>"IP Address: =
"</SPAN> + netif[<SPAN class=3Dhighlight-number>0</SPAN>].IPAddress);
                Debug.Print(<SPAN class=3Dhighlight-literal>"Subnet =
Mask: "</SPAN> + netif[<SPAN =
class=3Dhighlight-number>0</SPAN>].SubnetMask);
                Debug.Print(<SPAN class=3Dhighlight-literal>"Default =
Getway: "</SPAN> + netif[<SPAN =
class=3Dhighlight-number>0</SPAN>].GatewayAddress);
                Debug.Print(<SPAN class=3Dhighlight-literal>"DNS Server: =
"</SPAN> + netif[<SPAN =
class=3Dhighlight-number>0</SPAN>].DnsAddresses[<SPAN =
class=3Dhighlight-number>0</SPAN>]);
            }
            <SPAN class=3Dhighlight-keyword>catch</SPAN>
            {
                Debug.Print(<SPAN class=3Dhighlight-literal>"DHCP =
Faild"</SPAN>);
            }

            Debug.Print(<SPAN class=3Dhighlight-literal>"Test =
DNS"</SPAN>);
            <SPAN class=3Dhighlight-keyword>try</SPAN>
            {
                IPHostEntry myIP =3D Dns.GetHostEntry(<SPAN =
class=3Dhighlight-literal>"www.ghielectronics.com"</SPAN>);

                <SPAN class=3Dhighlight-keyword>if</SPAN> (myIP !=3D =
<SPAN class=3Dhighlight-keyword>null</SPAN>)
                {
                    Debug.Print(myIP.HostName + <SPAN =
class=3Dhighlight-literal>": "</SPAN> + myIP.AddressList[<SPAN =
class=3Dhighlight-number>0</SPAN>].ToString());
                }
            }
            <SPAN class=3Dhighlight-keyword>catch</SPAN>
            {
                Debug.Print(<SPAN class=3Dhighlight-literal>"Faild to =
Get the host entry of the FQN from DNS server!"</SPAN>);
            }

            Thread.Sleep(Timeout.Infinite);

        }

        <SPAN class=3Dhighlight-keyword>static</SPAN> <SPAN =
class=3Dhighlight-keyword>void</SPAN> =
NetworkChange_NetworkAvailabilityChanged(<SPAN =
class=3Dhighlight-keyword>object</SPAN> sender, =
NetworkAvailabilityEventArgs e)
        {
            <SPAN class=3Dhighlight-keyword>if</SPAN> (e.IsAvailable)
            {
                <SPAN class=3Dhighlight-keyword>if</SPAN> =
(Ethernet.IsCableConnected)
                {
                    <SPAN class=3Dhighlight-keyword>if</SPAN> =
(ethernet_last_status !=3D <SPAN class=3Dhighlight-keyword>true</SPAN>)
                    {
                        ethernet_last_status =3D <SPAN =
class=3Dhighlight-keyword>true</SPAN>;
                        NetworkAvailablityBlocking.Set();
                    }
                }
            }
            <SPAN class=3Dhighlight-keyword>else</SPAN>

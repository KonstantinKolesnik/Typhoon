<DIV id=3D"sourceCode"><PRE class=3D"brush:c#;" title=3D"Source =
code">/// &lt;summary&gt;=0A=
/// PWM implementation that uses the shadow registers for a clean =
transition between changing duty.=0A=
/// This is how it should be implemented in the GHI firmware :)=0A=
/// =0A=
/// Programmed by Huysentruit Wouter=0A=
/// Fastload-Media.be=0A=
/// &lt;/summary&gt;=0A=
public class ShadowPWM : IDisposable=0A=
{=0A=
    #region Consts=0A=
=0A=
    const uint PINSEL4 =3D 0xE002C000 + 0x10;=0A=
    const uint PINSEL7 =3D 0xE002C000 + 0x1C;=0A=
=0A=
    const uint PWM0_BASE_ADDR =3D 0xE0014000;=0A=
    const uint PWM0TCR =3D PWM0_BASE_ADDR + 0x04;=0A=
    const uint PWM0PR  =3D PWM0_BASE_ADDR + 0x0C;=0A=
    const uint PWM0MCR =3D PWM0_BASE_ADDR + 0x14;=0A=
    const uint PWM0MR0 =3D PWM0_BASE_ADDR + 0x18;=0A=
    const uint PWM0MR1 =3D PWM0_BASE_ADDR + 0x1C;=0A=
    const uint PWM0MR2 =3D PWM0_BASE_ADDR + 0x20;=0A=
    const uint PWM0MR3 =3D PWM0_BASE_ADDR + 0x24;=0A=
    const uint PWM0MR4 =3D PWM0_BASE_ADDR + 0x40;=0A=
    const uint PWM0MR5 =3D PWM0_BASE_ADDR + 0x44;=0A=
    const uint PWM0MR6 =3D PWM0_BASE_ADDR + 0x48;=0A=
    const uint PWM0PCR =3D PWM0_BASE_ADDR + 0x4C;=0A=
    const uint PWM0LER =3D PWM0_BASE_ADDR + 0x50;=0A=
=0A=
    const uint PWM1_BASE_ADDR =3D 0xE0018000;=0A=
    const uint PWM1TCR =3D PWM1_BASE_ADDR + 0x04;=0A=
    const uint PWM1PR  =3D PWM1_BASE_ADDR + 0x0C;=0A=
    const uint PWM1MCR =3D PWM1_BASE_ADDR + 0x14;=0A=
    const uint PWM1MR0 =3D PWM1_BASE_ADDR + 0x18;=0A=
    const uint PWM1MR1 =3D PWM1_BASE_ADDR + 0x1C;=0A=
    const uint PWM1MR2 =3D PWM1_BASE_ADDR + 0x20;=0A=
    const uint PWM1MR3 =3D PWM1_BASE_ADDR + 0x24;=0A=
    const uint PWM1MR4 =3D PWM1_BASE_ADDR + 0x40;=0A=
    const uint PWM1MR5 =3D PWM1_BASE_ADDR + 0x44;=0A=
    const uint PWM1MR6 =3D PWM1_BASE_ADDR + 0x48;=0A=
    const uint PWM1PCR =3D PWM1_BASE_ADDR + 0x4C;=0A=
    const uint PWM1LER =3D PWM1_BASE_ADDR + 0x50;=0A=
=0A=
    #endregion=0A=
=0A=
    #region Enums=0A=
=0A=
    internal enum Device=0A=
    {=0A=
        EMX=0A=
    }=0A=
=0A=
    #endregion=0A=
=0A=
    #region Pins=0A=
=0A=
    public class Pin=0A=
    {=0A=
        internal Device Device { get; private set; }=0A=
        internal byte Modulator { get; private set; }=0A=
        internal byte Channel { get; private set; }=0A=
            =0A=
        private Pin(Device device, byte modulator, byte channel)=0A=
        {=0A=
            Device =3D device;=0A=
            Modulator =3D modulator;=0A=
            Channel =3D channel;=0A=
        }=0A=
=0A=
        public static readonly Pin EmxPWM0 =3D new Pin(Device.EMX, 0, =
1);   // P3.16 / PWM0[1]=0A=
        public static readonly Pin EmxPWM1 =3D new Pin(Device.EMX, 1, =
1);   // P3.24 / PWM1[1]=0A=
        public static readonly Pin EmxPWM2 =3D new Pin(Device.EMX, 0, =
2);   // P3.17 / PWM0[2]=0A=
        public static readonly Pin EmxPWM3 =3D new Pin(Device.EMX, 1, =
3);   // P3.26 / PWM1[3]=0A=
        public static readonly Pin EmxPWM4 =3D new Pin(Device.EMX, 1, =
4);   // P3.27 / PWM1[4]=0A=
        public static readonly Pin EmxPWM5 =3D new Pin(Device.EMX, 1, =
6);   // P3.29 / PWM1[6]=0A=
    }=0A=
=0A=
    #endregion=0A=
=0A=
    #region Declarations=0A=
=0A=
    // Registers for modulator 0=0A=
    private static Register ler0 =3D new Register(PWM0LER);=0A=
    private static Register tcr0 =3D new Register(PWM0TCR);=0A=
    private static Register mr00 =3D new Register(PWM0MR0);=0A=
    private static Register pcr0 =3D new Register(PWM0PCR);=0A=
    private static Register mcr0 =3D new Register(PWM0MCR);=0A=
        =0A=
    // Registers for modulator 1=0A=
    private static Register ler1 =3D new Register(PWM1LER);=0A=
    private static Register tcr1 =3D new Register(PWM1TCR);=0A=
    private static Register mr10 =3D new Register(PWM1MR0);=0A=
    private static Register pcr1 =3D new Register(PWM1PCR);=0A=
    private static Register mcr1 =3D new Register(PWM1MCR);=0A=
=0A=
    // Declarations for pin=0A=
    private Pin pin;=0A=
    private Register ler =3D null;=0A=
    private Register mr0 =3D null;=0A=
    private Register mr =3D null;=0A=
=0A=
    private static object syncLock =3D new object();=0A=
=0A=
    #endregion=0A=
=0A=
    #region Construction / destruction=0A=
=0A=
    static ShadowPWM()=0A=
    {=0A=
        Register reg;=0A=
        reg =3D new Register(PWM0PR); reg.Write(0);=0A=
        reg =3D new Register(PWM1PR); reg.Write(0);=0A=
=0A=
        // reset=0A=
        tcr0.Write(0x02);   // Counter reset=0A=
        tcr1.Write(0x02);   // Counter reset=0A=
=0A=
        pcr0.Write(0x00);   // Disable all PWM outputs=0A=
        pcr1.Write(0x00);   // Disable all PWM outputs=0A=
=0A=
        mcr0.Write(0x02);   // Reset on PWMMR0=0A=
        mcr1.Write(0x02);   // Reset on PWMMR0=0A=
    }=0A=
=0A=
    public ShadowPWM(Pin pin)=0A=
    {=0A=
        this.pin =3D pin;=0A=
            =0A=
        Init();=0A=
    }=0A=
=0A=
    public void Dispose()=0A=
    {=0A=
        Deinit();=0A=
    }=0A=
=0A=
    #endregion=0A=
=0A=
    #region Private methods=0A=
=0A=
    private void Init()=0A=
    {=0A=
        lock (syncLock)=0A=
        {=0A=
            if (pin.Modulator =3D=3D 0)=0A=
            {=0A=
                // Enable PWM function on pin=0A=
                switch (pin.Device)=0A=
                {=0A=
                    case Device.EMX:=0A=
                        Register pinsel =3D new Register(PINSEL7);=0A=
                        int offset =3D (pin.Channel - 1) &lt;&lt; 1;=0A=
                        pinsel.ClearBits(0x03U &lt;&lt; offset);=0A=
                        pinsel.SetBits(0x02U &lt;&lt; offset);=0A=
                        break;=0A=
                    default:=0A=
                        throw new NotImplementedException();=0A=
                }=0A=
=0A=
                ler =3D ler0;=0A=
                mr0 =3D mr00;=0A=
                tcr0.Write(0x09); // Enable counter and PWM=0A=
=0A=
                switch (pin.Channel)=0A=
                {=0A=
                    case 1: mr =3D new Register(PWM0MR1); break;=0A=
                    case 2: mr =3D new Register(PWM0MR2); break;=0A=
                    case 3: mr =3D new Register(PWM0MR3); break;=0A=
                    case 4: mr =3D new Register(PWM0MR4); break;=0A=
                    case 5: mr =3D new Register(PWM0MR5); break;=0A=
                    case 6: mr =3D new Register(PWM0MR6); break;=0A=
                }=0A=
=0A=
                // Enable PWM output=0A=
                pcr0.SetBits(1U &lt;&lt; (8 + pin.Channel));=0A=
            }=0A=
            else=0A=
            {=0A=
                // Enable PWM function on pin=0A=
                switch (pin.Device)=0A=
                {=0A=
                    case Device.EMX:=0A=
                        Register pinsel =3D new Register(PINSEL7);=0A=
                        int offset =3D ((pin.Channel - 1) &lt;&lt; 1) + =
16;=0A=
                        uint temp =3D pinsel.Read();=0A=
                        pinsel.ClearBits(0x03U &lt;&lt; offset);=0A=
                        pinsel.SetBits(0x03U &lt;&lt; offset);=0A=
                        break;=0A=
                    default:=0A=
                        throw new NotImplementedException();=0A=
                }=0A=
=0A=
                ler =3D ler1;=0A=
                mr0 =3D mr10;=0A=
                tcr1.Write(0x09); // Enable counter and PWM=0A=
=0A=
                switch (pin.Channel)=0A=
                {=0A=
                    case 1: mr =3D new Register(PWM1MR1); break;=0A=
                    case 2: mr =3D new Register(PWM1MR2); break;=0A=
                    case 3: mr =3D new Register(PWM1MR3); break;=0A=
                    case 4: mr =3D new Register(PWM1MR4); break;=0A=
                    case 5: mr =3D new Register(PWM1MR5); break;=0A=
                    case 6: mr =3D new Register(PWM1MR6); break;=0A=
                }=0A=
=0A=
                // Enable PWM output=0A=
                pcr1.SetBits(1U &lt;&lt; (8 + pin.Channel));=0A=
            }=0A=
=0A=
            mr.Write(0);=0A=
            ler.Write(1U &lt;&lt; pin.Channel);=0A=
        }=0A=
    }=0A=
=0A=
    private void Deinit()=0A=
    {=0A=
        lock (syncLock)=0A=
        {=0A=
            if (pin =3D=3D null)=0A=
                throw new ObjectDisposedException();=0A=
=0A=
            if (pin.Modulator =3D=3D 0)=0A=
            {=0A=
                // Disable PWM output=0A=
                pcr0.ClearBits(1U &lt;&lt; (8 + pin.Channel));=0A=
=0A=
                // Disable PWM function on pin (select normal GPIO)=0A=
                switch (pin.Device)=0A=
                {=0A=
                    case Device.EMX:=0A=
                        Register pinsel =3D new Register(PINSEL7);=0A=
                        int offset =3D (pin.Channel - 1) &lt;&lt; 1;=0A=
                        pinsel.ClearBits(0x03U &lt;&lt; offset);=0A=
                        break;=0A=
                    default:=0A=
                        throw new NotImplementedException();=0A=
                }=0A=
            }=0A=
            else=0A=
            {=0A=
                // Disable PWM output=0A=
                pcr1.SetBits(1U &lt;&lt; (8 + pin.Channel));=0A=
=0A=
                // Disable PWM function on pin (select normal GPIO)=0A=
                switch (pin.Device)=0A=
                {=0A=
                    case Device.EMX:=0A=
                        Register pinsel =3D new Register(PINSEL7);=0A=
                        int offset =3D ((pin.Channel - 1) &lt;&lt; 1) + =
16;=0A=
                        uint temp =3D pinsel.Read();=0A=
                        pinsel.ClearBits(0x03U &lt;&lt; offset);=0A=
                        break;=0A=
                    default:=0A=
                        throw new NotImplementedException();=0A=
                }=0A=
            }=0A=
=0A=
            ler =3D null;=0A=
            mr =3D null;=0A=
            pin =3D null;=0A=
        }=0A=
    }=0A=
=0A=
    #endregion=0A=
=0A=
    #region Public methods=0A=
=0A=
    public void SetPulse(uint period_nanosecond, uint =
highTime_nanosecond)=0A=
    {=0A=
        // Adjust for 72 MHz / 4 clock=0A=
        period_nanosecond *=3D 9;=0A=
        period_nanosecond /=3D 500;=0A=
        highTime_nanosecond *=3D 9;=0A=
        highTime_nanosecond /=3D 500;=0A=
=0A=
        lock (syncLock)=0A=
        {=0A=
            if (pin =3D=3D null)=0A=
                throw new ObjectDisposedException();=0A=
=0A=
            mr0.Write(period_nanosecond);           // Set the clock=0A=
            mr.Write(highTime_nanosecond);          // Set duty=0A=

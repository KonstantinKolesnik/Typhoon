/*
** general portlet styles (elements in the quickbar)
*/
.portlet {
    border: none; 
    margin: 0 0 .5em;
    padding: 0;
    float: none;
    width: 11.6em;
     overflow: hidden; 
}
.portlet h4 {
    font-size: 95%;

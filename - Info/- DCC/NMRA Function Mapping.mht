var decvalue =3D "";

function calculateit(form) {
	calcvalue =3D Math.abs(form.D.value);
	if (calcvalue > 255) {
		alert("DEC > 0 & DEC < 256");
		form.D.value =3D "";
	} else {
		for ( i =3D 7 ; i >=3D 0 ; i-- ) {
			code =3D "form.B"+i+".value =3D Math.round(calcvalue/Math.pow(2,i)- =

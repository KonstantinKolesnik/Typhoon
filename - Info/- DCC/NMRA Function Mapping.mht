			eval(code);
			//alert (calcvalue + " " +i);
			code =3D "calcvalue =3D calcvalue - (Math.pow(2,i) * =
form.B"+i+".value)";
			eval(code);
		}
	}
}

function calculateback(form) {
decvalue=3D0;
	for ( var i =3D 0 ; i < 8 ; i++ ) {
		code =3D "decvalue =3D decvalue + Math.pow(2,i) * form.B"+i+".value";
		eval(code);
	}
	form.D.value =3D decvalue;
}


function clearbin(form)=20
{
    for (var i =3D 0; i < 8; i++) {
    	code =3D "form.B"+i+".value =3D ''";
    	eval(code);
    }
}

function cleardec(form) {
	form.D.value =3D "";
}

function transfer(chkbox) {
	bit =3D chkbox.name.substr(2,1);
	//alert (chkbox.checked);
	if (chkbox.checked =3D=3D true) {
		code =3D "document.Bin.B" + bit + ".value =3D 1";
		if (bit =3D=3D 3 || bit =3D=3D 6 || bit =3D=3D 7) {
			alert("This value should stay '0'\nIt is officially not used!");
		}
	} else {
		code =3D "document.Bin.B" + bit + ".value =3D 0";
	}
	eval(code);
	calculateback(document.Bin);
}

function mapping(chkbox) {
	cv =3D chkbox.name.substr(4,2);
	//alert(cv);
	if (chkbox.checked=3D=3Dtrue) {
		code =3D "document.Bin.CV" + cv + ".value =3D document.Bin.CV" + cv + =
".value * 1.0 + " + chkbox.value;
	} else {
		code =3D "document.Bin.CV" + cv + ".value =3D document.Bin.CV" + cv + =
".value * 1.0 - " + chkbox.value;
	}
	eval(code);
}


